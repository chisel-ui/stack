{
    "name": "@chisel-ui/stack",
    "version": "0.0.1",
    "description": "chisel-ui stack layout primitive",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "require": "./dist/index.js",
        "import": "./dist/index.mjs",
        "type": "./dist/index.d.ts"
      }
    },
    "scripts": {
      "build": "tsup src/index.ts --format cjs,esm --dts --clean",
      "watch": "npm run build -- --watch src",
      "prepublishOnly": "npm run build"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/chisel-ui/stack.git"
    },
    "keywords": [
      "stack",
      "layout"
    ],
    "author": "tony-sull <tony.f.sullivan@outlook.com>",
    "license": "MIT",
    "bugs": {
      "url": "https://github.com/chisel-ui/stack/issues"
    },
    "homepage": "https://github.com/chisel-ui/stack#readme",
    "devDependencies": {
      "tsup": "^5.12.1",
      "typescript": "^4.6.2"
    }
  }
  